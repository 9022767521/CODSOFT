{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "meta-viewport": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "width: fit-content"
        ]
      }
    ]
  }
}